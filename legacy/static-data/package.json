{
  "name": "@workspace/static-data",
  "version": "0.1.0",
  "private": true,
  "description": "Static game data for JDR Coffee applications",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./lib/currencies": "./src/lib/currencies/index.ts",
    "./lib/universes": "./src/lib/universes/index.ts",
    "./lib/periods": "./src/lib/periods/index.ts",
    "./lib/genres": "./src/lib/genres/index.ts",
    "./lib/categories": "./src/lib/categories/index.ts",
    "./lib/materials": "./src/lib/materials/index.ts",
    "./lib/rarities": "./src/lib/rarities/index.ts",
    "./lib/item-properties": "./src/lib/item-properties/index.ts",
    "./lib/equipment-slots": "./src/lib/equipment-slots/index.ts",
    "./lib/items": "./src/lib/items/index.ts",
    "./data/items": "./src/data/items/index.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "@workspace/typescript-config": "workspace:*",
    "typescript": "^5.3.3",
    "vitest": "^4.0.15"
  }
}
